{
  "name": "medusa-starter-default",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0",
    "npm": "DO NOT USE NPM - USE YARN INSTEAD",
    "yarn": ">=1.22.0"
  },
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "build": "medusa build",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "start": "medusa start",
    "dev": "nodemon",
    "predeploy": "medusa db:migrate --execute-safe-links",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit",
    "sync:meili": "medusa exec ./src/scripts/force-meili-sync.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.974.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@medusajs/admin-sdk": "^2.13.0",
    "@medusajs/cli": "^2.13.1",
    "@medusajs/file-s3": "^2.13.0",
    "@medusajs/framework": "^2.13.0",
    "@medusajs/icons": "^2.13.0",
    "@medusajs/js-sdk": "^2.13.0",
    "@medusajs/medusa": "^2.13.0",
    "@medusajs/ui": "^4.1.0",
    "@rokmohar/medusa-plugin-meilisearch": "^1.3.5",
    "@types/pg": "^8.16.0",
    "@woocommerce/woocommerce-rest-api": "^1.0.2",
    "dotenv": "^17.2.3",
    "medusa-plugin-auth": "^1.11.1",
    "meilisearch": "^0.55.0",
    "multer": "^2.0.2",
    "typeorm": "^0.3.16"
  },
  "devDependencies": {
    "@medusajs/test-utils": "^2.13.0",
    "@swc/core": "^1.7.28",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.13",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.12.11",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "jest": "^29.7.0",
    "nodemon": "^3.1.11",
    "pg": "^8.17.2",
    "postgres": "^3.4.8",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.6.2",
    "vite": "^5.4.14",
    "yalc": "1.0.0-pre.53"
  }
}
